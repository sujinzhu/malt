<div class="ui-helper-clearfix">
  #if(${isnew})
  <form action='${url.for("%{action='createcontacts'}")}' method="POST">
  #else
  <form action='${url.for("%{action='updatecontacts'}")}' method="POST">
  #end
  ${f.push("extension4")}
  ${f.hiddenfield("Id")}
