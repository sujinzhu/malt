#capturefor(head)
  ${j.jquery_dataTable()}
  <script type="text/javascript" language="javascript" charset="utf-8">
  // <![CDATA[
    $(function(){
      $('#finals').dataTable({
        "bJQueryUI": true,
        "sPaginationType": "full_numbers"
      });
    });
  // ]]>
  </script>
#end
#capturefor(title)
${common.releasematerial}
#end

<h3>${common.releasematerial}</h3>

<div id="header-container">
<table id="finals" class="display">
  <thead>
    <tr>
      <th>Id</th>
      <th>${common.workshop} / ${common.vendor}</th>
      <th>${common.applyby}</th>
      <th>${common.status}</th>
      <th>${common.createdate}</th>
    </tr>
  </thead>
  <tbody>
  #foreach($issuingNote in $finals)
    <tr>
      <td>${url.link($issuingNote.Id, "%{action='steering', querystring='id=$issuingNote.Id'}")}</td>
      <td> ${r.getdescription($issuingNote.IssueTo)} </td>
      <td>${issuingNote.ApplyBy}</td>
      <td>${h.getenumname($issuingNote.Status)}</td>
      <td>${issuingNote.CreateDate.ToShortDateString()}</td>
    </tr>
  #end
  </tbody>
</table>
</div>
